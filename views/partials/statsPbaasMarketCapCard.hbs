<div class="card" id="stats-pbaas-marketcap-card">
    <img width="16px" src="/img/stats.png" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">PBaaS Marketcap</div>
        {{!-- {{#if vrscOnline}} --}}
        {{#each marketCapArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{this.blockchain}}}</div>
            <div class="card-row-item">
                $ {{{this.marketCap}}}
            </div>
        </div>
        {{/each}}

    </div>
</div>