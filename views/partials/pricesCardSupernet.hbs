<div class="card" id="prices-card-supernet">
    <img width="16px" src="/img/vsupernet.png" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">SUPERNET</div>
        {{#if vrscOnline}}
        {{#each supernetPriceArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{origin}}}</div>
            <div class="card-row-item-network">{{{network}}}</div>
            <div class="card-row-item">
                $ {{{price}}}
            </div>
        </div>
        {{/each}}
        <br>

        <div class="card-row-wrapper">
            <div class="card-row-item">SUPERNET in baskets</div>
            <div class="card-row-item">
                {{{supernetReserve}}}
            </div>
        </div>

        <div class="card-row-wrapper">
            <div class="card-row-item">SUPERNET value in baskets</div>
            <div class="card-row-item">
                $ {{{supernetReserveValue}}}
            </div>
        </div>
        {{else}}
        {{{vrscStatusMessage}}}
        {{/if}}
    </div>
</div>