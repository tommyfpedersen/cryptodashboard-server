{{#each currencyGroupList}}


{{!-- <div>
    <div>{{this.basketName}} </div>
    <div>{{this.basketType}} </div>
    <div>{{this.basketName}} </div>
</div> --}}
 {{#ifEquals currencyType "Token"}} 
<div class="card" id="prices-card-{{currencyAlias}}">
    <img width="16px" src="{{currencyIcon}}" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">{{currencyAlias}}</div>
        {{!-- {{#if vrscOnline}} --}}
        {{#each this.currencyList}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{this.basketName}}}</div>
            <div class="card-row-item-network">{{{this.currencyNetwork}}}</div>
            <div class="card-row-item">
                $ {{{toFixed this.currencyPriceUSD 2}}}
            </div>
        </div>
        {{/each}}
        <br>

        <div class="card-row-wrapper">
            <div class="card-row-item">{{currencyAlias}} in baskets</div>
            <div class="card-row-item">
                {{{currencySupply}}}
            </div>
        </div>

         {{#if currencyNote}}
         <br>
        <div class="card-row-wrapper">
           <div class="card-row-item">{{currencyNote}}</div>
        </div>
        
        {{/if}}

        {{!-- {{else}}
        {{{vrscStatusMessage}}}
        {{/if}} --}}
    </div>
</div>
{{/ifEquals}}
{{/each}}

