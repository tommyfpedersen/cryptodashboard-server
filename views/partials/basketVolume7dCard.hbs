<div class="card" id="{{blockchain}}-basket-volume-7d">
    <img width="16px" src="{{icon}}" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">Basket Volume 7D</div>

        {{#each this.basketVolume7DaysList}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{name}}}</div>
            <div class="card-row-item-network"></div>
            <div class="card-row-item">
                $ {{{basketVolumeValueUSD}}}
            </div>
        </div>
        {{/each}}
        <br>
        <div class="card-row-wrapper">
            <div class="card-row-item"> Total volume</div>
            <div class="card-row-item" id="total-volume-7d">
                $ {{basketVolume7DaysUSD}}
            </div>
        </div>



    </div>
</div>