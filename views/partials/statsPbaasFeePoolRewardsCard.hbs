<div class="card" id="stats-pbaas-fee-pool-rewards-card">
    <img width="16px" src="/img/stats.png" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">PBaaS Fee Pool Rewards</div>
        {{!-- {{#if vrscOnline}} --}}
        {{#each feePoolRewardArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{this.blockchain}}}</div>
            <div class="card-row-item">
                 {{{this.feeReward}}}
            </div>
        </div>
        {{/each}}

    </div>
</div>