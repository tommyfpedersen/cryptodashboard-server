{{#each currencyArray}}
<div class="currency-card">
    <img width="16px" src="/img/tbtc.png" /> tBTC (Bitcoin)

    <div class="card-content">
        <div class="card-title">Bitcoin</div>

        {{#each this.priceArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item"> {{this.name}}</div>
            <div class="card-row-item-network">vrsc</div>
            <div class="card-row-item">
                $ 2
            </div>
        </div>


        {{/each}}

        {{!-- {{#if vrscOnline}}
        {{#each btcPriceArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{origin}}}</div>
            <div class="card-row-item-network">{{{network}}}</div>
            <div class="card-row-item">
                $ {{{price}}}
            </div>
        </div>
        {{/each}}
        <br>

        <div class="card-row-wrapper">
            <div class="card-row-item">Bitcoin in baskets</div>
            <div class="card-row-item">
                {{{btcReserve}}}
            </div>
        </div>

        <div class="card-row-wrapper">
            <div class="card-row-item">Bitcoin value in baskets</div>
            <div class="card-row-item">
                $ {{{btcReserveValue}}}
            </div>
        </div>
        {{else}}
        {{{vrscStatusMessage}}}
        {{/if}} --}}
    </div>

</div>
{{/each}}