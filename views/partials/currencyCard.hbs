{{#each currencyGroupList}}
<div class="currency-card">
    <img width="16px" src="/img/tbtc.png" /> {{currencyName}}

    <div class="card-content">
        <div class="card-title">{{currencyName}}</div>

        {{#each this.currencyList}}
        <div class="card-row-wrapper">
            <div class="card-row-item"> {{this.basketName}}</div>
            <div class="card-row-item-network">{{this.currencyNetwork}}</div>
            <div class="card-row-item"> $ {{this.currencyPriceUSD}} </div>
        </div>


        {{/each}}

        {{!-- {{#if vrscOnline}}
        {{#each btcPriceArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{origin}}}</div>
            <div class="card-row-item-network">{{{network}}}</div>
            <div class="card-row-item">
                $ {{{price}}}
            </div>
        </div>
        {{/each}}
        <br>

        <div class="card-row-wrapper">
            <div class="card-row-item">Bitcoin in baskets</div>
            <div class="card-row-item">
                {{{btcReserve}}}
            </div>
        </div>

        <div class="card-row-wrapper">
            <div class="card-row-item">Bitcoin value in baskets</div>
            <div class="card-row-item">
                $ {{{btcReserveValue}}}
            </div>
        </div>
        {{else}}
        {{{vrscStatusMessage}}}
        {{/if}} --}}
    </div>

</div>
{{/each}}