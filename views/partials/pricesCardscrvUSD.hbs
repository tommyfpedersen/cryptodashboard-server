<div class="card" id="prices-card-scrvusd">
    <img width="16px" src="/img/scrvusd.webp" />
    <button class="hide-button">x</button>
    <div class="card-content">
        <div class="card-title">scrvUSD</div>
        {{#if vrscOnline}}
        {{#each scrvUSDPriceArray}}
        <div class="card-row-wrapper">
            <div class="card-row-item">{{{origin}}}</div>
            <div class="card-row-item-network">{{{network}}}</div>
            <div class="card-row-item">
                $ {{{price}}}
            </div>
        </div>
        {{/each}}
        <br>

        <div class="card-row-wrapper">
            <div class="card-row-item">scrvUSD in baskets</div>
            <div class="card-row-item">
                {{{scrvUSDReserve}}}
            </div>
        </div>

        <div class="card-row-wrapper">
            <div class="card-row-item">scrvUSD value in baskets</div>
            <div class="card-row-item">
                $ {{{scrvUSDReserveValue}}}
            </div>
        </div>
        {{else}}
        {{{vrscStatusMessage}}}
        {{/if}}
    </div>
</div>