        {{> varrrGetAddressBalanceCard}}


        <div class="card" id="verus-varrr-block-and-fee-pool-rewards-card">
            <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" />
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Block and Fee Pool Rewards</div>
                {{#if varrrOnline}}

                <div class="card-row-wrapper">
                    <div class="card-row-item">Block Height</div>
                    <div class="card-row-item"> {{{varrrblocks}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Last Block Send</div>
                    <div class="card-row-item"> {{{varrrblockLastSend}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Block Reward</div>
                    <div class="card-row-item"> {{{varrrblockReward}}} vARRR</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Fee Pool Reward</div>
                    <div class="card-row-item"> {{{varrrfeeReward}}} vARRR</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Average Block Fees</div>
                    <div class="card-row-item"> {{{varrraverageblockfees}}} vARRR</div>
                </div>

                {{else}}
                {{{varrrStatusMessage}}}
                {{/if}}

            </div>

        </div>

        <div class="card" id="varrr-staking-rewards-card">
            <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" />
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Staking Rewards</div>
                {{#if varrrOnline}}

                <div class="card-sub-title">Staking Amount</div>
                <div class="card-sub-title-value"><input id="varrr-staking-amount" class="card-input"
                        value="{{{varrrStakingAmount}}}"></div>

                {{#each varrrStakingRewardsArray}}
                <div class="card-row-wrapper">
                    <div class="card-row-item">{{{label}}}</div>
                    <div class="card-row-item-network">{{{rewards}}} vARRR</div>
                    <div class="card-row-item"> $ {{{dollars}}} </div>
                </div>

                {{/each}}
                <br>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Staking APY</div>
                    <div class="card-row-item"> {{{varrrStakingAPY}}} %</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Staking Supply</div>
                    <div class="card-row-item"> {{{varrrStakingSupply}}} </div>
                </div>

                {{else}}
                {{{varrrStatusMessage}}}
                {{/if}}

            </div>
        </div>

        <div class="card" id="varrr-mining-rewards-card">
            <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" />
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Mining Rewards</div>
                {{#if varrrOnline}}

                <div class="card-sub-title">MH/s</div>
                <div class="card-sub-title-value"><input id="varrr-mining-hash" class="card-input"
                        value="{{{varrrMiningHash}}}"></div>

                {{#each varrrMiningRewardsArray}}
                <div class="card-row-wrapper">
                    <div class="card-row-item">{{{label}}}</div>
                    <div class="card-row-item-network">{{{rewards}}} vARRR</div>
                    <div class="card-row-item"> $ {{{dollars}}} </div>
                </div>

                {{/each}}

                <br>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Network Hashrate</div>
                    <div class="card-row-item"> {{{varrrNetworkHash}}} GH/s </div>
                </div>

                {{else}}
                {{{varrrStatusMessage}}}
                {{/if}}

            </div>
        </div>



        <div class="card" id="verus-varrr-bridge-veth-volume-basket-card">
            <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" />
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Bridge.vARRR Basket</div>
                {{#if varrrOnline}}


                <div class="graph-container">
                    <div class="graph-group hide">
                        <div class="card-sub-title">24 hours Volume</div>
                        <div class="card-sub-title-main-value"> {{{varrrBridgeVolumeInDollars24Hours}}} <span
                                class="card-sub-title-value-prefix">VRSC</span> </div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each varrrBridgeVolumeInDollars24HoursArrayYAxis}}<div class="graph-y-axis-item">
                                {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each varrrBridgeVolumeInDollars24HoursArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-large" data-volume="{{volume}}" data-label="">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>


                    <div class="graph-group hide">
                        <div class="card-sub-title">7 days Volume</div>
                        <div class="card-sub-title-main-value"> {{{varrrBridgeVolumeInDollars7Days}}} <span
                                class="card-sub-title-value-prefix">VRSC</span></div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each varrrBridgeVolumeInDollars7DaysArrayYAxis}}<div class="graph-y-axis-item">
                                {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each varrrBridgeVolumeInDollars7DaysArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-xlarge" data-volume="{{volume}}" data-label="">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>


                    <div class="graph-group">
                        <div class="card-sub-title">30 days Volume</div>
                        <div class="card-sub-title-main-value">{{{varrrBridgeVolumeInDollars30Days}}} <span
                                class="card-sub-title-value-prefix">VRSC</span></div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each varrrBridgeVolumeInDollars30DaysArrayYAxis}}<div class="graph-y-axis-item">
                                {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each varrrBridgeVolumeInDollars30DaysArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-small" data-volume="{{volume}}" data-label="">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>

                    <div class="graph-button-group">
                        <button class="graph-button button-deselected">24H</button><button
                            class="graph-button button-deselected">7D</button><button
                            class="graph-button button-selected">30D</button>
                    </div>
                </div>

                <br>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Reserves Value</div>
                    <div class="card-row-item-network">via BTC</div>
                    <div class="card-row-item"> $ {{{estimatedVarrrBridgeReserveValueUSDBTC}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Reserves Value</div>
                    <div class="card-row-item-network">via VRSC</div>
                    <div class="card-row-item"> $ {{{estimatedVarrrBridgeReserveValueUSDVRSC}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Bridge.vARRR</div>
                    <div class="card-row-item-network">VRSC {{estimatedVarrrBridgeValueVRSC}}</div>
                    <div class="card-row-item"> $ {{{estimatedVarrrBridgeValueUSD}}}</div>
                </div>

                {{#each currencyVarrrBridgeArray}}
                <div style="text-transform:none;" class="card-sub-title">{{currencyName}}</div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Reserve</div>
                    <div class="card-row-item"> {{reserves}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">{{pricelabel}} Price</div>
                    <div class="card-row-item"> {{priceNative}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Basket Price</div>
                    <div class="card-row-item"> $ {{price}}</div>
                </div>

                <div class="card-row-wrapper">
                    <div class="card-row-item">{{coingeckoLabel}} Price</div>
                    <div class="card-row-item"> $ {{coingeckoprice}} </div>
                </div>

                {{/each}}


                {{else}}
                {{{varrrStatusMessage}}}
                {{/if}}
            </div>
        </div>





