                    <div class="card-row-item">Market cap</div>
                    <div class="card-row-item">$ {{{marketCap}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Fully diluted market cap</div>
                    <div class="card-row-item">$ {{{fullyDilutedMarketCap}}} </div>
                </div>
                <br>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Circulating supply percentage</div>
                    <div class="card-row-item"> {{{circulatingSupplyPercentage}}} %</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Circulating supply</div>
                    <div class="card-row-item"> {{{circulatingSupply}}}</div>
                </div>
                {{!-- <div class="card-row-wrapper">
                    <div class="card-row-item">Total supply</div>
                    <div class="card-row-item"> {{{totalSupply}}} </div>
                </div> --}}
                <div class="card-row-wrapper">
                    <div class="card-row-item">Max. supply</div>
                    <div class="card-row-item"> {{{maxSupply}}} </div>
                </div>
                <br>

                <div class="card-row-wrapper">
                    <div class="card-row-item">Staking APY</div>
                    <div class="card-row-item"> {{{stakingAPY}}} %</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Staking percentage</div>
                    <div class="card-row-item"> {{{stakingPercentage}}} %</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Staking supply</div>
                    <div class="card-row-item"> {{{stakingSupply}}}</div>
                </div>
                {{!-- <div class="card-row-wrapper">
                    <div class="card-row-item">Staking value</div>
                    <div class="card-row-item"> $ {{{stakingValue}}} </div>
                </div> --}}

                {{!-- <div class="card-row-item-details">Last 100 blocks</div> --}}
                {{else}}
                {{{vrscStatusMessage}}}
                {{/if}}
            </div>
        </div>

