        <span class="project" id="vdex" data-text="vDEX"></span>
        <div class="card" id="verus-vdex-get-address-balance-card">
            {{!-- <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" /> --}}
            vDEX
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Address Balance</div>
                {{#if vdexOnline}}

                <div class="card-sub-title">vDEX address</div>
                <div class="card-sub-title-value"><input id="get-vdex-address-balance" class="card-input"
                        value="{{{getVdexAddress}}}"></div>

                {{#each getVdexAddressBalanceArray}}
                <div class="card-row-wrapper">
                    <div class="card-row-item">{{{currencyName}}}</div>
                    <div class="card-row-item">{{{amount}}} </div>
                </div>
                {{/each}}

                {{else}}
                {{{vdexStatusMessage}}}
                {{/if}}

            </div>
        </div>

        <div class="card" id="verus-vdex-block-and-fee-pool-rewards-card">
            {{!-- <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" /> --}}
            vDEX
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Block and Fee Pool Rewards</div>
                {{#if vdexOnline}}

                <div class="card-row-wrapper">
                    <div class="card-row-item">Block Height</div>
                    <div class="card-row-item"> {{{vdexblocks}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Last Block Send</div>
                    <div class="card-row-item"> {{{vdexblockLastSend}}}</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Block Reward</div>
                    <div class="card-row-item"> {{{vdexblockReward}}} vDEX</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Fee Pool Reward</div>
                    <div class="card-row-item"> {{{vdexfeeReward}}} vDEX</div>
                </div>
                <div class="card-row-wrapper">
                    <div class="card-row-item">Average Block Fees</div>
                    <div class="card-row-item"> {{{vdexaverageblockfees}}} vDEX</div>
                </div>

                {{else}}
                {{{vdexStatusMessage}}}
                {{/if}}

            </div>

        </div>

        <div class="card" id="vdex-staking-rewards-card">
            {{!-- <img width="16px" src="/img/Pirate_Logo_Coin_Gold.png" /> --}}
            vDEX
            <button class="hide-button">x</button>
            <div class="card-content">
                <div class="card-title">Staking Rewards</div>
                {{#if vdexOnline}}

                <div class="card-sub-title">Staking Amount</div>
                <div class="card-sub-title-value"><input id="vdex-staking-amount" class="card-input"
                        value="{{{vdexStakingAmount}}}"></div>

