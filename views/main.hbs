            <div class="card-content">
                {{#if online}}
                <div class="card-title">Block and Fee Pool Rewards</div>

                <div class="card-sub-title">block height</div>
                <div id="verus-block-height" class="card-sub-title-value"> {{{blocks}}} </div>
                <div class="card-sub-details">{{{blockLastSend}}}</div>

                <div class="card-sub-title">block reward</div>
                <div id="verus-block-reward" class="card-sub-title-value"> {{{blockReward}}} VRSC</div>

                <div class="card-sub-title">fee pool reward</div>
                <div id="verus-fee-reward" class="card-sub-title-value"> {{{feeReward}}} VRSC</div>

                <div class="card-sub-title">average block fees (fee pool size)</div>
                <div id="verus-average-fee-pool-size" class="card-sub-title-value">{{{averageblockfees}}} VRSC</div>
                <div class="card-sub-details">Calculated from the last 100 blocks</div>

                {{else}}
                {{{statusMessage}}}
                {{/if}}

            </div>

        </div>
        <div class="card" id="verus-bridge-veth-basket-volume-card">
            <svg fill="#3165d4" width="55" height="16" viewBox="0 0 4950 1417" xmlns="http://www.w3.org/2000/svg"
