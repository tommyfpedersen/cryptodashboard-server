            <button class="hide-button">x</button>
            <div class="card-content">
                {{#if online}}
                <div class="card-title">Get Verus Address Balance</div>

                <div class="card-sub-title">Verus address</div>
                <div class="card-sub-title-value"><input id="get-address-balance" class="card-input"
                        value="{{{getAddress}}}"></div>

                {{#each getAddressBalanceArray}}
                <div style="text-transform:none;" class="card-sub-title">{{currencyName}}</div>
                <div class="card-sub-title-value"> {{amount}} </div>
                {{/each}}

                {{else}}
                {{{statusMessage}}}
                {{/if}}

            </div>
        </div>
        <div class="card" id="verus-block-and-fee-pool-rewards-card">
            <svg fill="#3165d4" width="55" height="16" viewBox="0 0 4950 1417" xmlns="http://www.w3.org/2000/svg"
