        {{> verusBasketVolume24hCard}}
        {{> verusBlockAndFeePoolRewardsCard}}
        {{> verusStakingRewardsCard }}
        {{> verusMiningRewardsCard }}
        {{> verusBridgeVethVolumeBasketCard }}

        {{> verusKaijuVolumeBasketCard }}
        {{> verusNatiVolumeBasketCard }}
        {{> verusPureVolumeBasketCard }}
        {{> verusSwitchVolumeBasketCard }}


