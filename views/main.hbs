            <button class="hide-button">x</button>
            <div class="card-content">
                {{#if online}}
                {{!-- <div class="card-title">VRSC-ETH Bridge Volume</div> --}}
                <div class="card-title">Bridge.vETH Basket Volume</div>


                <div class="graph-container">
                    <div class="graph-group hide">
                        <div class="card-sub-title">24 hours Volume</div>
                        <div class="card-sub-title-value">$ {{{vrscBridgeVolumeInDollars24Hours}}} </div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each vrscBridgeVolumeInDollars24HoursArrayYAxis}}<div class="graph-y-axis-item">
                                $ {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each vrscBridgeVolumeInDollars24HoursArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-large" data-price="{{price}}" data-label="{{label}}">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>


                    <div class="graph-group hide">
                        <div class="card-sub-title">7 days Volume</div>
                        <div class="card-sub-title-value">$ {{{vrscBridgeVolumeInDollars7Days}}} </div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each vrscBridgeVolumeInDollars7DaysArrayYAxis}}<div class="graph-y-axis-item">
                                $ {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each vrscBridgeVolumeInDollars7DaysArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-xlarge" data-price="{{price}}" data-label="{{label}}">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>


                    <div class="graph-group">
                        <div class="card-sub-title">30 days Volume</div>
                        <div class="card-sub-title-value">$ {{{vrscBridgeVolumeInDollars30Days}}} </div>
                        <br>
                        <div class="graph-y-axis">
                            {{#each vrscBridgeVolumeInDollars30DaysArrayYAxis}}<div class="graph-y-axis-item">
                                $ {{value}}
                            </div>{{/each}}
                        </div>

                        <div class="graph-bar-container">
                            {{#each vrscBridgeVolumeInDollars30DaysArray}}<div style="height:{{barPCT}}px;"
                                class="graph-bar graph-bar-small" data-price="{{price}}" data-label="{{label}}">
                            </div>{{/each}}
                            <div class="graph-x-axis"></div>
                        </div>
                    </div>

                    <div class="graph-button-group">
                        <button class="graph-button button-deselected">24H</button><button
                            class="graph-button button-deselected">7D</button><button
                            class="graph-button button-selected">30D</button>
                    </div>
                </div>

                <br>
                <div class="card-sub-details">Tracking VRSC, DAI.vETH, MKR.vETH and vETH reserve in and out</div>
                {{else}}
                {{{statusMessage}}}
                {{/if}}
            </div>
        </div>
        <div class="card" id="verus-bridge-veth-basket-card">
            <svg fill="#3165d4" width="55" height="16" viewBox="0 0 4950 1417" xmlns="http://www.w3.org/2000/svg"
