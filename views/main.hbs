                <div class="card-sub-title-value"><input id="get-address-balance" class="card-input"
                        value="{{{getAddress}}}"></div>

                {{#each getAddressBalanceArray}}
                <div class="card-row-wrapper">
                    <div class="card-row-item">{{{currencyName}}}</div>
                    <div class="card-row-item">
                        {{{amount}}}
                    </div>
                </div>

                {{/each}}


